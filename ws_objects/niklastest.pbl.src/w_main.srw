public function longlong convert_numeral (integer i_type, longlong i_value_source);integer i_item
integer i_loop
integer i_system_limit = 200

long i_value_remainder
long i_convert_cache
long i_convert_result
long i_source

boolean bool_null_event

string a_str_value_store []
string a_str_null []

string str_remainder_cache
string str_convert_result

i_source = i_value_source
For i_loop = 1 To 3
i_convert_cache = i_source
	//Convert to Binary
	if i_loop = 1 then
		For i_item = 1 To i_system_limit
			//fast convert for single digit situation
			if i_source = 1 Then
				bool_null_event = True
				str_result_bin = "1"
				Exit
			elseif i_source = 0 Then
				bool_null_event = True
				str_result_bin = "0"
				Exit
			End if
			i_value_remainder = Mod(i_convert_cache, 2)
			i_convert_cache = i_convert_cache / 2
			if i_value_remainder = 0 Then
				a_str_value_store [i_item] = "0"
			elseif i_value_remainder = 1 Then
				a_str_value_store [i_item] = "1"
			End if
			
			//break if 0
			if i_convert_cache = 0 Then
				Exit
			End if
		Next
		if bool_null_event = True Then Continue
	End if

	
	//Convert to Hexadecimal
	if i_loop = 2 Then
		For i_item = 1 To i_system_limit
			if bool_source_hex = True Then 
				Exit
			End if
			//if number is smaller than 16, convert directly
			if i_source < 16 Then 
				a_str_value_store [i_item] = direct_convert_hex(i_convert_cache, False, "")
				Exit
			End if
			i_value_remainder = Mod(i_convert_cache, 16)
			i_convert_cache = i_convert_cache / 16
			if i_convert_cache > 15 Then
				a_str_value_store [i_item] = direct_convert_hex(i_value_remainder, False, "")
			End if
			if i_convert_cache < 16 Then
				a_str_value_store [i_item] = direct_convert_hex(i_value_remainder, False, "")
				a_str_value_store [i_item + 1] = direct_convert_hex(i_convert_cache, False, "")
				Exit
			End if
		Next
	End if
	
	//Convert to Octal
	if i_loop = 3 Then
		For i_item = 1 To i_system_limit
			if i_convert_cache < 8 Then 
				a_str_value_store [i_item] = string(i_convert_cache)
				Exit
			end if
			if bool_source_oct = True Then 
				Exit
			End if
			i_value_remainder = Mod(i_convert_cache, 8)
			i_convert_cache = i_convert_cache / 8
			a_str_value_store [i_item] = string(i_value_remainder)
		Next
	End if
	
	//Read Array Backwards for result
	for i_item = UpperBound(a_str_value_store) To 1 Step -1
		if str_convert_result = "" Then
			str_convert_result = a_str_value_store[i_item]
		elseif Not str_convert_result = "" Then
			str_convert_result = str_convert_result + a_str_value_store[i_item]
		End if
	Next
	
	//save to Binary result
	if i_loop = 1 then
		str_result_bin = str_convert_result
	End if
	
	//save to Hexadecimal result
	if i_loop = 2 Then	
		str_result_hex = str_convert_result
	End if
	
	//save to octal result
	if i_loop = 3 Then	
		str_result_oct = str_convert_result
	End if
	a_str_value_store = a_str_null
	str_convert_result = ""
Next
Return 1
end function

public function string clean_input (string input);integer iUpper_limit
integer iItem
string saStringCache[]
string str_input
string str_output
string str_cache

str_output = ""

//deconstruct string
for iItem = 1 To len(input)
	saStringCache[iItem] = mid(input, iItem, 1)
Next

if i_active_numsystem = 2 Then
//check for character other than 0 or 1 in binary
	for iItem = 1 to upperbound(saStringCache)
		if saStringCache[iItem] = "1" Then
			str_output = str_output + "1"
		elseif saStringCache[iItem] = "0" Then
			str_output = str_output + "0"
		End if
	Next
	tab_frame_1.tabpage_numerals.sle_bin.Text = str_output
	
Elseif i_active_numsystem = 3 Then
//Check for letters/symbols other than A to F or 0 to 9 in Hex
	for iItem = 1 to upperbound(saStringCache)
		str_input = saStringCache[iItem]
		str_cache = ""
		if str_input = "A" Then
			str_cache = "A"	
		elseif str_input = "a" Then
			str_cache = "A"
		elseif str_input = "B" Then
			str_cache =  "B"
		elseif str_input = "b" Then
			str_cache =  "B"
		elseif str_input = "C" Then
			str_cache = "C"
		elseif str_input = "c" Then
			str_cache = "C"
		elseif str_input = "D" Then
			str_cache = "D"		
		elseif str_input = "d" Then
			str_cache = "D"
		elseif str_input = "E" Then
			str_cache = "E"
		elseif str_input = "e" Then
			str_cache = "E"
		elseif str_input = "F" Then
			str_cache = "F"
		elseif str_input = "f" Then
			str_cache = "F"
		elseif str_input = "0" Then
			if Not len(str_output) = 0 Then
				str_cache = "0"
			End if
		elseif IsNumber(str_input) = True Then
			str_cache = str_input
		Else
			str_cache = ""
		End if
		str_output = str_output + str_cache
	Next
	tab_frame_1.tabpage_numerals.sle_hex.Text = str_output
	
Elseif i_active_numsystem = 4 Then
//Check for character other than 0 to 7 in Oct
	for iItem = 1 to upperbound(saStringCache)
		str_input = saStringCache[iItem]
		if str_input = "1" Then
			str_cache = "1"
		elseif str_input = "2" Then
			str_cache = "2"
		elseif str_input = "3" Then
			str_cache = "3"
		elseif str_input = "4" Then
			str_cache = "4"
		elseif str_input = "5" Then
			str_cache = "5"
		elseif str_input = "6" Then
			str_cache = "6"
		elseif str_input = "7" Then
			str_cache = "7"
		elseif str_input = "0" Then
			if Not len(str_output) = 0 Then
				str_cache = "0"
			End if
		else
			str_cache = ""
		End if
		str_output = str_output + str_cache
	Next
	tab_frame_1.tabpage_numerals.sle_oct.Text = str_output
end if

return str_output
end function

public function string convert_to_decimal (integer i_active, string str_non_decimal_value);string a_str_converter_store []
string a_str_hex_word_store []
string a_str_null []

string str_source
string str_operation_cache
string str_display_cache

long i_number_cache
long i_operation_cache
long i_result_cache
long i_string_len

integer i_item

str_source = str_non_decimal_value
i_string_len = Len(str_source)
//if binary
if i_active = 2 Then
	
	For i_item = Len(str_source) To 1 Step -1
		str_operation_cache = Mid(str_source, i_item, 1)
		i_number_cache = long(str_operation_cache) * 2^(i_string_len - i_item)
		if str_operation_cache = "0" Then
			a_str_converter_store [i_item] = "0"
		else
			a_str_converter_store [i_item] = string(i_number_cache)
		End if
	Next
	
	For i_item = UpperBound(a_str_converter_store) To 1 Step -1
		i_operation_cache = i_operation_cache + long(a_str_converter_store [i_item])
	Next
	
	i_result_cache = i_operation_cache
	i_operation_cache = 0
	a_str_converter_store = a_str_null
	tab_frame_1.tabpage_numerals.st_debug_1.Text = string(i_result_cache)
	Return string(i_result_cache)

//if hex
elseif i_active = 3 then
	
	For i_item = Len(str_source) To 1 Step -1
		str_operation_cache = Mid(str_source, i_item, 1)
		a_str_hex_word_store [i_item] = str_operation_cache
		str_operation_cache = direct_convert_hex(0, True, str_operation_cache)
		i_number_cache = long(str_operation_cache) * 16^(i_string_len - i_item)
		if str_operation_cache = "0" Then
			a_str_converter_store [i_item] = "0"
		else
			a_str_converter_store [i_item] = string(i_number_cache)
		End if
	Next
	
	//values to decimal
	For i_item = UpperBound(a_str_converter_store) To 1 Step -1
		i_operation_cache = i_operation_cache + long(a_str_converter_store [i_item])
	Next
	
	i_result_cache = i_operation_cache
	i_operation_cache = 0
	a_str_converter_store = a_str_null
	tab_frame_1.tabpage_numerals.st_debug_1.Text = string(i_result_cache)
	Return string(i_result_cache)

//if octal
elseif i_active = 4 Then
	
	For i_item = Len(str_source) To 1 Step -1
		str_operation_cache = Mid(str_source, i_item, 1)
		i_number_cache = long(str_operation_cache) * 8^(i_string_len - i_item)
		if str_operation_cache = "0" Then
			a_str_converter_store [i_item] = "0"
		else
			a_str_converter_store [i_item] = string(i_number_cache)
		End if
	Next
	
	For i_item = UpperBound(a_str_converter_store) To 1 Step -1
		i_operation_cache = i_operation_cache + long(a_str_converter_store [i_item])
	Next
	
	i_result_cache = i_operation_cache
	i_operation_cache = 0
	a_str_converter_store = a_str_null
	tab_frame_1.tabpage_numerals.st_debug_1.Text = string(i_result_cache)
	Return string(i_result_cache)
End if
end function

