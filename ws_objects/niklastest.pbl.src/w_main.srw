public subroutine primes (longlong l_limit, longlong l_start);integer index
longlong item
longlong loops
longlong loops_2
longlong l_primes
longlong lastprime
longlong next_nonprime
longlong primestep
string str_primes
string str_item
string str_item_2
string str_space

l_primes = 1

tab_frame_1.tabpage_Primes.mle_primes.Text = ""

tab_frame_1.tabpage_Primes.hpb_primes.MaxPosition = l_limit
tab_frame_1.tabpage_Primes.hpb_primes.Position = 0

for item = 2 to l_limit
	b_a_primes_filter[item] = False
Next

for item = 2 to sqrt(l_limit)
	yield()
	tab_frame_1.tabpage_Primes.hpb_primes.Position = item
	if Not b_a_primes_filter[item] Then
		b_a_primes_final[item] = True
		//l_primes++
		primestep = item
		next_nonprime = item*item
		for loops = item * item to l_limit
			if loops = next_nonprime Then
				b_a_primes_filter[next_nonprime] = True
				next_nonprime = next_nonprime + primestep
			End if
		Next
	End if
	
Next

for item = sqrt(l_limit) + 1 to l_limit
	tab_frame_1.tabpage_Primes.hpb_primes.Position = item
	if Not b_a_primes_filter[item] Then
		b_a_primes_final[item] = True
		//l_primes++
		lastprime = item
	End if
Next

//Old output function for listbox
//index = 1
//for item = l_start to upperbound(b_a_primes_final)
//	if b_a_primes_final[item] = True Then
//		if Not item = 1 Then
//			str_item = string(item)
//			tab_frame_1.tabpage_1.lb_primes.InsertItem (str_item, index)
//			index++
//		End if
//	End if
//	tab_frame_1.tabpage_Primes.hpb_primes.Position = item
//Next

l_primes = 0
//Writes Primes to a String and counts them based on the start-number
for item = l_start to lastprime
	if b_a_primes_final[item] Then
		if Not item = 1 Then
			str_item_2 = str_item_2 + "[" + string(item) + "]  "
		End if
		if Not item < l_start Then
			l_primes++
		End if
	End if
Next
tab_frame_1.tabpage_Primes.hpb_primes.Position = l_limit - 1
tab_frame_1.tabpage_Primes.st_prime_count.Text = string(l_primes)
tab_frame_1.tabpage_Primes.mle_primes.Text = str_item_2

end subroutine

public function integer add_memory (double d_value);integer index
string name

index = i_memory_slots +1
name = "memory_" + string(index)
tab_frame_1.tabpage_mem_new.ddlb_memory_list.insertItem(name, index)

