public function integer convert_units (double input);//Conversion to ref unit
if i_active_unit_type = 1 Then //Länge
	if i_active_input_unit = 1 Then //Nanometer
		d_unit_cache = input / 10000000 
	elseif i_active_input_unit = 2 Then //Mikrometer
		d_unit_cache = input / 10000
	elseif i_active_input_unit = 3 Then //Millimeter
		d_unit_cache = input / 10
	elseif i_active_input_unit = 4 Then //Zentimeter
		d_unit_cache = input
	elseif i_active_input_unit = 5 Then //Meter
		d_unit_cache = input * 100
	elseif i_active_input_unit = 6 Then //Kilometer
		d_unit_cache = input * 100000
	elseif i_active_input_unit = 7 Then //Zoll
		d_unit_cache = input * 2.54
	elseif i_active_input_unit = 8 Then //Fuß
		d_unit_cache = input * 30.48
	elseif i_active_input_unit = 9 Then //Yard
		d_unit_cache = input * 91.44
	elseif i_active_input_unit = 10 Then //Meilen
		d_unit_cache = input * 160934.4
	elseif i_active_input_unit = 11 Then //Seemeilen
		d_unit_cache = input * 185200
	End if
