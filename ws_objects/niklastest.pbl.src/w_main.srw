string name
index = i_current_index
if Not i_memory_slots = 0 Then
	if Not index = 0 Then
		if Not index = i_memory_slots Then
			index_cache = i_current_index
		Elseif index = i_memory_slots Then
			index_cache = i_current_index - 1
		Elseif index = 1 Then
			index_cache = i_current_index
		End if
		index = index_cache
		i_memory_slots --
	
		clear_memory(i_current_index)
		if Not upperbound(str_a_memory_names) = 0 Then
			name = str_a_memory_names[index]
		End if
		tab_frame_1.tabpage_mem_new.ddlb_memory_list.DeleteItem(i_current_index)
		tab_frame_1.tabpage_mem_new.ddlb_memory_list.SelectItem(name, index)
		change_memory(index)
	End if
End if
tab_frame_1.tabpage_mem_new.st_memory_count.Text = string(i_memory_slots)

if i_memory_slots = 0 Then
	b_no_memory = True
end if
