public function string direct_convert_hex (integer i_input, boolean bool_is_hex, string str_input);string str_cache
if bool_is_hex = False Then
	//direct convert to Hex
	if i_input < 10 Then
		str_cache = string(i_input)
	elseif i_input > 9 Then
		if i_input = 10 Then
			str_cache = "A"
		elseif i_input = 11 Then
			str_cache = "B"
		elseif i_input = 12 Then
			str_cache = "C"
		elseif i_input = 13 Then
			str_cache = "D"
		elseif i_input = 14 Then
			str_cache = "E"
		elseif i_input = 15 Then
			str_cache = "F"
		End if
	End if
elseif bool_is_hex = True Then
	if IsNumber(str_input) = True Then
		if longlong(str_input) < 10 Then
			str_cache = str_input
		End if
	else
		if str_input = "A" Then
			str_cache = "10"		
		elseif str_input = "a" Then
			str_cache = "10"
		elseif str_input = "B" Then
			str_cache = "11"
		elseif str_input = "b" Then
			str_cache = "11"
		elseif str_input = "C" Then
			str_cache = "12"
		elseif str_input = "c" Then
			str_cache = "12"
		elseif str_input = "D" Then
			str_cache = "13"		
		elseif str_input = "d" Then
			str_cache = "13"
		elseif str_input = "E" Then
			str_cache = "14"
		elseif str_input = "e" Then
			str_cache = "14"
		elseif str_input = "F" Then
			str_cache = "15"
		elseif str_input = "f" Then
			str_cache = "15"
		End if
	End if
End if
Return str_cache
						
					
end function

