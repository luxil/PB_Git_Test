public function double calc_formula (integer formula_id, double d_1, double d_2, double d_3, double d_4, double d_5, double d_6);double d_formula_result
double d_formula_cache

if d_1 < 0 Then d_1 = d_1 * -1
if d_1 < 0 Then d_2 = d_2 * -1
if d_1 < 0 Then d_3 = d_3 * -1
if d_1 < 0 Then d_4 = d_4 * -1
if d_1 < 0 Then d_5 = d_5 * -1
if d_1 < 0 Then d_6 = d_6 * -1

if formula_id = 1 Then //Kreis 1
	d_formula_result =d_const_pi * (d_1 ^ 2.0)
elseif formula_id = 2 Then //Kreis 2
	d_formula_result =  2.0 * d_const_pi * d_1
elseif formula_id = 3 Then //Kreisring 1
	d_formula_result =  d_const_pi * (d_2 ^ 2.0 - d_1 ^ 2.0)
elseif formula_id = 4 Then //Kreisring 2
	d_formula_result =  2.0 * d_const_pi * (d_2 + d_1)
elseif formula_id = 5 Then //Quadrat 1
	d_formula_result =  d_1^2.0
elseif formula_id = 6 Then //Quadrat 2
	d_formula_result =  4.0 * d_1
elseif formula_id = 7 Then //Rechteck 1
	d_formula_result = d_1 * d_2
elseif formula_id = 8 Then //Rechteck 2
	d_formula_result = 2.0 * ((d_1 * d_2) + (d_1 * d_3) + (d_2 * d_3))
elseif formula_id = 9 Then //Würfel 1
	d_formula_result = d_1^3.0
elseif formula_id = 10 Then //Würfel 2
	d_formula_result = 6.0 * (d_1^2.0)
elseif formula_id = 11 Then //Quader 1
	d_formula_result = d_1 * d_2 * d_3
elseif formula_id = 12 Then //Quader 2
	d_formula_result = 2.0 * ((d_1 * d_2) + (d_1 * d_3) + (d_2 * d_3))
elseif formula_id = 13 Then //Zylinder 1
	d_formula_result = d_const_pi * (d_1^2.0) * d_2
elseif formula_id = 14 Then //Zylinder 2
	d_formula_result = (2.0 * d_const_pi * d_1) * (d_1 + d_2)
elseif formula_id = 15 Then //Pyramide 1
	d_formula_result = (1.0 / 3.0) * d_1 * d_2
elseif formula_id = 16 Then //Pyramide 2
	d_formula_cache = 4.0 * (sqrt((d_2 ^ 2.0) + ((d_3 ^ 2.0) / 4.0)))
	d_formula_result = d_1 + d_formula_cache
elseif formula_id = 17 Then //Kugel 1
	d_formula_result = d_const_pi * (d_1^2.0) * d_1
elseif formula_id = 18 Then //Kugel 2
	d_formula_result = 4.0 * d_const_pi * (d_1^2.0)
elseif formula_id = 19 Then //Torus 1
	d_formula_result = 2.0 * (d_const_pi ^ 2.0) * d_2 * (d_1^2.0)
elseif formula_id = 20 Then //Torus 2
	d_formula_result = 4.0 * (d_const_pi ^ 2.0) * d_2 * d_1
End if
if d_formula_result < 0 Then d_formula_result = d_formula_result * -1
Return d_formula_result
end function

