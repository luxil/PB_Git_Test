public function integer convert_units_new (integer unit_type, double input, integer array_index_input, integer array_index_output);//Convert to Reference Unit
if unit_type = 1 Then //Länge
	d_unit_cache = input * a_d_length_units [array_index_input]
elseif unit_type = 2 Then //Fläche
	d_unit_cache = input * a_d_area_units [array_index_input]
elseif unit_type = 3 Then //Volumen
	d_unit_cache = input * a_d_volume_units [array_index_input]
elseif unit_type = 4 Then //Geschwindigkeit
	d_unit_cache = input * a_d_velocity_units [array_index_input]
elseif unit_type = 5 Then //Zeit
	d_unit_cache = input * a_d_time_units [array_index_input]
elseif unit_type = 6 Then //Gewicht
	d_unit_cache = input * a_d_mass_units [array_index_input]
End if

//Convert to Target Unit
if unit_type = 1 Then //Länge
	d_unit_conversion_result = d_unit_cache / a_d_length_units [array_index_output]
elseif unit_type = 2 Then //Fläche
	d_unit_conversion_result = d_unit_cache / a_d_area_units [array_index_output]
elseif unit_type = 3 Then //Volumen
	d_unit_conversion_result = d_unit_cache / a_d_volume_units [array_index_output]
elseif unit_type = 4 Then //Geschwindigkeit
	d_unit_conversion_result = d_unit_cache / a_d_velocity_units [array_index_output]
elseif unit_type = 5 Then //Zeit
	d_unit_conversion_result = d_unit_cache / a_d_time_units [array_index_output]
elseif unit_type = 6 Then //Zeit
	d_unit_conversion_result = d_unit_cache / a_d_mass_units [array_index_output]

End if
Return 1
end function

