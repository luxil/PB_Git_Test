public function integer set_num_sum (string num_button);bool_operated = False
bool_number_given = True
if bool_number_is_const = True Then
	str_input_1 = "0"
	bool_number_is_const = False
End if
if str_input_1 = "0" Then
	str_input_1 = num_button
elseif Not str_input_1 = "0" Then
	str_input_1 =  str_input_1 + num_button
End if
sle_prime_output.Text = str_input_1
Return 1
end function

public function integer move_sum ();if bool_operated = True Then
	str_input_1 = str_last_result
	sle_second_output.Text = str_last_result
End if
if bool_operator_given = True Then
	operate (str_last_operator, str_input_1, str_input_2)
	str_input_2 = str_result
	sle_second_output.Text = string(str_input_2)
	str_input_1 = "0"
	sle_prime_output.Text = "0"
	str_last_operator = str_operator
elseif bool_operator_given = False Then
	str_input_2 = str_input_1
	sle_second_output.Text = string(str_input_2)
	str_input_1 = "0"
	sle_prime_output.Text = "0"
	str_last_operator = str_operator
	bool_operator_given = True
End if

Return 1
end function

public function integer operate (string operator, string input, string second_input);string str_loc_operator_symbol
string str_loc_result_cache
string str_loc_operation_cache
d_input_1 = double (input)
d_input_2 =  double (second_input)
if operator = "addition" Then
	d_result = d_input_2 + d_input_1
	str_loc_operator_symbol = " + "
elseif operator = "substraction" Then
	d_result = d_input_2 - d_input_1
	str_loc_operator_symbol = " - "
elseif operator = "division" Then
	if Not d_input_1 = 0 Then
		d_result = d_input_2 / d_input_1
		str_loc_operator_symbol = " / "
	End if	
elseif operator = "multiplication" Then
	d_result = d_input_2 * d_input_1
	str_loc_operator_symbol = " * "
elseif operator = "exponential" Then
	d_result = d_input_2 ^ d_input_1
	str_loc_operator_symbol = " ^ "
	/*if d_result < 0 Then
		d_result = d_result * -1
	End if*/
End if
str_result = string(d_result)
str_loc_result_cache = str_result
str_loc_operation_cache = second_input + str_loc_operator_symbol + input
update_history_datawindow(str_loc_result_cache, str_loc_operation_cache)
if Not str_input_1 = "" Then
	str_last_input = str_input_1
End if
str_last_result = str_result
return 1
end function

